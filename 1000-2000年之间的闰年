int main()
{
	int i;
	int count = 0;
	for (i = 1000; i <= 2000; i++)
	{
		if (i % 4 == 0 && i%100 !=0)
		{
			printf("1000-2000年之间的闰年：%d\n", i);
			count++;
		}
		else if (i % 400 == 0)
		{
			printf("1000-2000年之间的闰年：%d\n", i);
			count++;
		}
		
	}
	printf("1000-2000年之间的闰年数量为：%d\n", count);
	return 0;
}
