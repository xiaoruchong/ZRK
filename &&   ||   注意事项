int main()
{
	int i = 0, a = 0, b = 2, c = 3, d = 4;
	//i = a++ && ++b && d++;//a++   先使用，后++ ，所以左边a为0，后面的都
	//不会执行了
	i = a++ || ++b || d++;//a为0，b为非零，满足逻辑或，d不用执行了

	printf("a = %d\n b = %d\n c = %d\n d = %d\n", a, b, c, d);
	//输出 1 2 3 4 
	//int a = 0;
	//int b = 5;
	//int c = a && b;//两个都是非0，则为1；反之为0；逻辑与
	//int d = a || b;//有一个为非0，则为1，反之为0；逻辑或
	return 0;
}
